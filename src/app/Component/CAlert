import React, { Component } from 'react';
import  swal from  'sweetalert2';
//codigo para un alerta
class CAlert extends Component {
    showAlert() {
        swal.fire({
            title: 'Alert',
            text: 'Registro exitoso',
            icon: 'warning',
            confirmButtonText: 'OK'
        });
    }

    render() {
        return (this.showAlert());
    }
}

export default CAlert;
